{
  "name": "@karelkangro/deno-ai-toolkit",
  "version": "1.7.3",
  "description": "Complete functional toolkit for building AI-powered Deno servers with vector databases and agent orchestration",
  "license": "MIT",
  "exports": "./mod.ts",
  "tasks": {
    "dev": "deno run --allow-all --watch mod.ts",
    "test": "deno test --allow-all",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check mod.ts",
    "compile": "deno compile --allow-all mod.ts"
  },
  "imports": {
    "vectordb": "npm:@lancedb/lancedb@^0.17.0",
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/testing": "jsr:@std/testing@^1.0.0",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.0.0",
    "@aws-sdk/s3-request-presigner": "npm:@aws-sdk/s3-request-presigner@^3.0.0",
    "pdf-parse": "npm:pdf-parse@^2.4.3",
    "pdfjs-dist": "npm:pdfjs-dist@^4.0.379"
  },
  "compilerOptions": {
    "lib": ["deno.window", "deno.unstable"],
    "strict": true
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": false,
    "proseWrap": "preserve"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": [
        "no-explicit-any",
        "no-unused-vars",
        "require-await",
        "no-import-prefix"
      ]
    }
  }
}
