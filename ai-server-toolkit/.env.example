# Anthropic API Configuration
# API version for Anthropic Claude API requests
# Default: 2023-06-01
# See: https://docs.anthropic.com/en/api/versioning
ANTHROPIC_API_VERSION=2023-06-01

# Optional: API version for requests with prompt caching support
# If not set, uses ANTHROPIC_API_VERSION
ANTHROPIC_API_VERSION_WITH_CACHING=2023-06-01

# Anthropic LLM Default Configuration
# Default model to use if not specified in LLMConfig
# Default: claude-3-sonnet-20240229
ANTHROPIC_DEFAULT_MODEL=claude-3-sonnet-20240229

# Default max tokens for responses if not specified in LLMConfig
# Default: 4096
ANTHROPIC_DEFAULT_MAX_TOKENS=4096

# Default temperature for responses if not specified in LLMConfig
# Default: 0.7
ANTHROPIC_DEFAULT_TEMPERATURE=0.7

# Rate Limiting Configuration
# Requests per minute limit
# Default: 50
ANTHROPIC_RATE_LIMIT_PER_MINUTE=50

# Requests per hour limit
# Default: 1000
ANTHROPIC_RATE_LIMIT_PER_HOUR=1000
